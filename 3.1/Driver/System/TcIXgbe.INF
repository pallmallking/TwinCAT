ServiceType     = 1 ;%SERVICE_KERNEL_DRIVER%
StartType       = 2 ;%SERVICE_AUTO_START%
ErrorControl    = 1 ;%SERVICE_ERROR_NORMAL%
ServiceBinary   = %12%\TCIXGBE.sys
LoadOrderGroup  = NDIS

[common.EventLog]
AddReg = common.AddEventLog.reg

[common.AddEventLog.reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll"
HKR, , TypesSupported,   0x00010001, 7

[TCIXGBE.CopyFiles]
;TCIXGBE.sys,,,2

[SourceDisksNames]
1 = "TwinCAT Driver Disk 1",TCIXGBE.sys,,

[SourceDisksFiles]
TCIXGBE.sys = 1,,
;TcI8xxCo.dll = 1,,

[DestinationDirs]
TCIXGBE.CopyFiles         = 12
DefaultDestDirs            = 11

[Strings]
Intel                      = "Intel"
Bkhf                       = "Beckhoff"

TCIXGBE.DeviceDesc        = "TwinCAT-Intel PCI Ethernet Adapter (10 Gigabit)"
TCIXGBE.Service.DispName  = "TwinCAT based 10 Gigabit Ethernet Adapter NT Driver"

ReceiveFrameDescriptors    = "Receive Frame Descriptors"
TransmitControlBlocks      = "Transmit Control Blocks"
NumCoalesceBuffers         = "Coalesce Buffers"
TxThreshold				   = "Transmit Threshold"
TxPacing				   = "Transmit Pacing Speed"
JumboPacket				   = "Jumbo Packet"

AutoNegotiation             = "Auto Negotiation"
SpeedDuplex                 = "Link Speed & Duplex"
100Mb_Full_Duplex           = "100 Mbps Full Duplex"
1000Mb_Full_Duplex          = "1.0 Gbps Full Duplex"
2500Mb_Full_Duplex          = "2.5 Gbps Full Duplex"
5000Mb_Full_Duplex          = "5.0 Gbps Full Duplex"
10000Mb_Full_Duplex         = "10.0 Gbps Full Duplex"

Disabled                    = "Disabled"
Bytes4088					= "4088 Bytes"
Bytes9014					= "9014 Bytes"

