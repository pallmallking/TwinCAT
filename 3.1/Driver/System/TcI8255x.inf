; Intel 8255x Based PCI Ethernet Adapter (10/100) and Intel compatibles
;
; Windows NT specific entry
[TCI8255X.ndi.NT]
Characteristics = 0x84
BusType         = 5
AddReg          = 82557.reg, TCI8255X.reg
CopyFiles       = TCI8255X.CopyFiles
DelReg          = TcI8255xDel.reg

[TCI8255X.ndi.NT.Services]
AddService = TCI8255X, 2, TCI8255X.Service, common.EventLog

;-----------------------------------------------------------------------------
; common base for 10/100Mb Adapters
;
;-----------------------------------------------------------------------------
; 82557 common
;
[82557.reg]
HKR, ,                          NumRfd,     0, "256"
HKR, ,                          NumTcb,     0, "256"


HKR, Ndi\params\NumRfd,         ParamDesc,  0, "%ReceiveFrameDescriptors%"
HKR, Ndi\params\NumRfd,         default,    0, "256"
HKR, Ndi\params\NumRfd,         min,        0, "32"
HKR, Ndi\params\NumRfd,         max,        0, "4096"
HKR, Ndi\params\NumRfd,         step,       0, "1"
HKR, Ndi\params\NumRfd,         Base,       0, "10"
HKR, Ndi\params\NumRfd,         type,       0, "int"

HKR, Ndi\params\NumTcb,         ParamDesc,  0, "%TransmitControlBlocks%"
HKR, Ndi\params\NumTcb,         default,    0, "256"
HKR, Ndi\params\NumTcb,         min,        0, "32"
HKR, Ndi\params\NumTcb,         max,        0, "4096"
HKR, Ndi\params\NumTcb,         step,       0, "1"
